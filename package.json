{
  "name": "@lifarl/react-scroll-snap-slider",
  "version": "2.0.5",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "yarn panda && parcel ./examples/index.html --target pages",
    "build": "rm -rf ./dist && yarn panda && rollup -c",
    "build:pages": "rm -rf ./dist && yarn panda && parcel build ./examples/index.html --target pages --public-url https://lifarl.github.io/react-scroll-snap-slider/",
    "deploy:pages": "gh-pages -d dist",
    "test": "jest",
    "panda": "panda codegen",
    "prepare": "panda codegen",
    "postinstall": "panda codegen"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifarl/react-scroll-snap-slider.git"
  },
  "homepage": "https://lifarl.github.io/react-scroll-snap-slider",
  "targets": {
    "pages": {
      "context": "browser",
      "distDir": "dist"
    }
  },
  "alias": {
    "styled-system": "./styled-system"
  },
  "keywords": [
    "react",
    "carousel",
    "scroll snapping",
    "scroll snap",
    "scroll",
    "slider"
  ],
  "author": "lifarl",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@pandacss/dev": "^0.47.1",
    "@pandacss/preset-panda": "^0.47.1",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.0.10",
    "parcel": "^2.10.3",
    "prettier": "^2.0.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rollup": "^4.21.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=16.8.3",
    "react-dom": ">=16.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "packageManager": "yarn@4.9.4",
  "lint-staged": {
    "./src/*.{ts,tsx}": [
      "npx prettier --write",
      "eslint --fix"
    ]
  }
}
